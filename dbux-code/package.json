  "__main": "./src/index.js",
  "main": "./dist/bundle.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "dbuxViewContainer",
          "title": "Dbux",
          "icon": "resources/bug.svg"
        }
      ]
