{
  "name": "dbux-code",
  "private": true,
  "displayName": "Dbux (ALPHA)",
  "version": "0.1.28-dev.0",
  "publisher": "Domi",
  "description": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/Domiii/dbux"
  },
  "engines": {
    "vscode": "^1.46.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
